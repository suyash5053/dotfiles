{
  "search.exclude": {
    "**/node_modules": true
  },
  "editor.fontSize": 17,
  // "search.smartCase": true,
  "editor.formatOnSave": true,
  "editor.fontFamily": "Cascadia Code NF",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'clig', 'dlig', 'liga', 'calt'",
  "editor.snippetSuggestions": "inline",
  "editor.tabCompletion": "on",
  "terminal.integrated.fontFamily": "monospace",
  "editor.inlayHints.enabled": "on",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.format.defaultFormatter.pug": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.postcss": "prettier",
  "vetur.format.defaultFormatter.scss": "prettier",
  "vetur.format.defaultFormatter.less": "prettier",
  "vetur.format.defaultFormatter.stylus": "stylus-supremacy",
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.format.defaultFormatter.sass": "sass-formatter",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "emmet.includeLanguages": {
    "rust": "html"
  },
  "gitlens.gitCommands.closeOnFocusOut": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.productIconTheme": "fluent-icons",
  "json.maxItemsComputed": 20000,
  "dart.flutterSdkPath": "/Users/realm/flutter",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[rust]": {
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "jinxdash.prettier-rust"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Join sequence"
  ],
  "editor.inlineSuggest.enabled": true,
  "dart.openDevTools": "flutter",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "editor.stickyScroll.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "importCost.fontStyle": "italic",
  "editor.tokenColorCustomizations": {},
  "cSpell.enabledLanguageIds": [
